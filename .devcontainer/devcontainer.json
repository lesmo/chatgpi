{
    "name": "elizaos-dev",
    "dockerFile": "Dockerfile",
    "build": {
        "args": {
            "NODE_VER": "23.5.0",
            "PNPM_VER": "9.15.2"
        }
    },
    "privileged": true,
    "runArgs": [
        "-p 3000:3000",
        "-p 5173:5173",
        "--gpus=all",
        "--device=/dev/dri"
    ],
    "containerEnv": {},
    "customizations": {
        "vscode": {
            "extensions": [
                "vscode.json-language-features",
                "vscode.css-language-features",
                "ms-python.python"
            ]
        }
    },
    "features": {}
}
